<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="UI API Regression Suite">
    <test name="RegressionSuite">
        <classes>
            <class name="com.gits.herokuapp.tests.InvalidPassword">
                <methods>
                    <include name="invalidPassword" />
                </methods>
            </class>
            <class name="com.gits.herokuapp.tests.InvalidUsername">
                <methods>
                    <exclude name="" />
                </methods>
            </class>
            <class name="com.gits.herokuapp.tests.InvalidUsernamePassword">
                <methods>
                    <exclude name="" />
                </methods>
            </class>
            <class name="com.gits.herokuapp.tests.ValidUsernamePassword">
                <methods>
                    <exclude name="" />
                </methods>
            </class>
        </classes>
    </test>
</suite>